<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">    

	<ui:with field='msgs' type='vajracode.calocal.client.i18n.I18nConstants' />
	<ui:with field='res' type='vajracode.calocal.client.resources.Resources' />
	
	<ui:style>
	.root {
		min-height: 100%;
	}
	.main {
		width: 99%;
		text-align: start;
		max-width: 1250px;
		margin-top: 10px;
	}
	</ui:style>
	
	<g:HTMLPanel width="100%" ui:field="root" addStyleNames="{style.root}">		
		<center>
			<m:MaterialPanel ui:field="main" addStyleNames="{style.main}" />
		</center>			
    </g:HTMLPanel>
</ui:UiBinder>