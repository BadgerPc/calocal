<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	
	<ui:with field='res' type='vajracode.calocal.client.resources.Resources' />
	
	<ui:style>
		.btn{
			width: 200px;
		}
	</ui:style>
	
	<m:MaterialPanel>
		<m:MaterialModalContent ui:field="content">			
		</m:MaterialModalContent>
		
		<m:MaterialModalFooter ui:field="footer" 
			addStyleNames="{res.css.flexHorizontal} {res.css.justifyContentEnd}">			
		</m:MaterialModalFooter>
	
	</m:MaterialPanel>
</ui:UiBinder> 